<cftry><!--- populate resort dropdown ---><cfif request.src_domain eq 'S'>	<cfparam name="url.rst_code" default="SMB">	<cfinvoke component="components.general.resorts" method="getSandalsResorts" returnvariable="getResorts"></cfinvoke><cfelseif request.src_domain eq 'B'>	<cfparam name="url.rst_code" default="BNG">	<cfinvoke component="components.general.resorts" method="getBeachesResorts" returnvariable="getResorts"></cfinvoke></cfif>	<cfinvoke component="components.sandalsadmin.uk.utilities" method="sortQuery" returnvariable="getResorts">	<cfinvokeargument name="sortBy" value="resort_type">	<cfinvokeargument name="sortDir" value="desc">	<cfinvokeargument name="qryToSort" value="#getResorts#"></cfinvoke><!--- get rates for one resort, or all if no rst_code ---><cfif isDefined('rst_code') and rst_code neq 'null'>	<cfinvoke component="components.sandalsadmin.uk.rates" method="getResortRates" returnvariable="rateList">		<cfinvokeargument name="resort_code" value="#rst_code#">		<cfinvokeargument name="is_active" value="YES">	</cfinvoke><cfelse>	<cfinvoke component="components.sandalsadmin.uk.rates" method="getRates" returnvariable="rateList"></cfinvoke></cfif>								<table width="500" cellpadding="0" cellspacing="0" border="0" align="center" style="margin-top:20px; "><tr>	<td>		<form name="ratesForm" action="uk_rates.cfm">		<div style="height:30px;" class="tablehead">		<select name="rst_code" onChange="if(this.value != ''){document.ratesForm.submit();}" class="textbox" style="margin:5px; float:right; ">			<option value="">Select A Resort</option>			<cfoutput query="getResorts.results">				<cfif rst_code neq 'RP'>					<option value="#getResorts.results.rst_code#" <cfif isDefined('url.rst_code') and url.rst_code eq getResorts.results.rst_code>selected</cfif>>#resort_shortname#</option>				</cfif>			</cfoutput>		</select>		</div>		</form>	</td></tr></table><cfoutput><div class="gradhead" style="border-bottom:none; ">#rateList.results.resort_shortname#</div></cfoutput><table border="0" width="500" cellpadding="0" cellspacing="0" style="border:1px solid #999; margin-bottom:20px;"><cfif rateList.results.recordCount gt 0>	<cfoutput query="rateList.results" group="schedule_id">		<tr>			<td colspan="3" align="center">			<div class="subhead_orange" style="margin:5px;">			For Stays from #DateFormat(start_date,'dd-mmm-yy')# to #DateFormat(end_date,'dd-mmm-yy')#*</div></td>		</tr>		<tr bgcolor="##DEDEDE">			<td><div style="margin:5px 5px 5px 10px; "><strong>Category Name</strong></div> </td>			<td><div align="right" style="margin:5px; "><strong>7  Nights</strong></div></td>			<td><div align="right" style="margin:5px 10px 5px 5px; "><strong>14 Nights</strong></div></td>		</tr>		<cfoutput>			<tr <cfif currentRow MOD 2 eq 0>id="bgwhite"</cfif>>				<td><div style="width:346px; margin:4px 4px 4px 10px;">#category_name#</div></td>				<td align="right"><div style="width:75px; margin:4px">&pound;#NumberFormat(rateList.results.price_7nights,'_.__')#</div></td>				<td align="right"><div style="width:75px; margin:4px 10px 4px 4px;;">&pound;#NumberFormat(rateList.results.price_14nights,'_.__')#</div></td>			</tr>		</cfoutput>	</cfoutput><cfelse>	<tr>		<td align="center">Please Select A Resort</td>	</tr></cfif></table><div align="center">			<div style="padding:10px; clear:both;">				* Prices given are as a guide only, based on a particular Tour				  Operator, as at Sept 06. Please note that prices may differ				  according to season and operator, please contact Sandals for an accurate				  quote			</div></div><cfcatch type="any">	<cfinclude template="logError.cfm">	<cfabort></cfcatch></cftry>